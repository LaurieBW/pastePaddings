{
  "name": "Paste Paddings",
  "description": "Allows you to copy the paddings of one element and apply them to another",
  "version": "2.0.0",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "paste-paddings",
    "manifest": "src/manifest.json",
    "main": "{{ slug }}.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  },
  "author": "simonlidesign <liyujunsimon@gmail.com>",
  "repository": "https://github.com/simonlidesign/sketchPluginPastePaddings.git"
}
